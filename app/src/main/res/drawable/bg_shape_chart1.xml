<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <gradient
        android:endColor="#ffffff"
        android:gradientRadius="50"
        android:startColor="#fcfcfc"
        android:type="radial"
        />

    <stroke
        android:dashGap="5dp"
        android:dashWidth="0dp"
        android:width="2dp"
        android:color="#0b1d0b"
        />

    <corners
        android:radius="5dp"
        ></corners>

    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"
        />

</shape>